<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_tutor_details"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="edu.orangecoastcollege.cs273.kfrederick5tmorrissey1ischenck.occstudentsuccesscenter.TutorDetailsActivity"
    android:orientation="vertical">

    <TextView
        android:text="Tutor Name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tutorNameTextView"/>

    <TextView
        android:text="Tutor Subjects"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tutorSubjectsTextView"/>

    <TextView
        android:text="Tutor Classes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tutorClassTextView"/>

    <TextView
        android:text="Tutor Hours"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tutorHoursTextView"/>
</LinearLayout>
